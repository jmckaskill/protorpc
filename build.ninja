SRC_NINJA = src.ninja
INCLUDES = -I . -I gtest/googletest/include -I gtest/googletest
TOOLCHAIN_DIR = toolchain
SRC = .

include toolchain/host-gcc.ninja

build bin/$HOST/protoc.exe: copy /usr/local/bin/protoc

subninja generate.ninja
subninja toolchain/target-gcc-debug.ninja
subninja toolchain/target-gcc-release.ninja

default gcc-debug gcc-release
